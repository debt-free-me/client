{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Resource": [
        "arn:aws:logs:ap-southeast-2:{{aws-account-id}}:log-group:/aws/codebuild/debtfreemeclient-build",
        "arn:aws:logs:ap-southeast-2:{{aws-account-id}}:log-group:/aws/codebuild/debtfreemeclient-build:*"
      ],
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ]
    },
    {
      "Effect": "Allow",
      "Resource": [
        "arn:aws:s3:::debtfreemeclient-codepipeline-ap-southeast-2-*"
      ],
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:GetObjectVersion",
        "s3:GetBucketAcl",
        "s3:GetBucketLocation"
      ]
    }
  ]
}